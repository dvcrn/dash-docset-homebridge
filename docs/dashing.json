{
    "name": "Homebridge",
    "package": "homebridge",
    "index": "index.html",
    "selectors": {
        "h1": [
            {
                "type": "Package",
                "matchpath": "service_.*\\.html"
            },
            {
                "type": "Property",
                "matchpath": "characteristic_.*\\.html"
            },
            {
                "type": "Guide",
                "matchpath": "api_.*\\.html"
            }
        ],
        "h3": [
            {
                "type": "Section",
                "matchpath": "characteristic_.*\\.html"
            },
            {
                "type": "Section",
                "matchpath": "service_.*\\.html"
            },
            {
                "type": "Method",
                "matchpath": "api_.*\\.html"
            }
        ]
    },
    "ignore": ["ABOUT"],
    "icon32x32": "icon@2x.png",
    "allowJS": true,
    "ExternalURL": "https://github.com/dvcrn/dash-docset-homebridge"
}
